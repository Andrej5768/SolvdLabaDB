<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.buildingCompany.persistence.dao.ISalaryRepository">
    <select id="getSalaryById" parameterType="java.lang.Long" resultType="Salary">
        SELECT *
        FROM salaries
        WHERE id = #{id};
    </select>
</mapper>