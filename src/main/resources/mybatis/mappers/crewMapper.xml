<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.buildingCompany.persistence.dao.ICrewRepository">
    <select id="getCrewById" parameterType="java.lang.Long" resultType="Crew">
        SELECT *
        FROM crews
        WHERE id = #{id};
    </select>
</mapper>