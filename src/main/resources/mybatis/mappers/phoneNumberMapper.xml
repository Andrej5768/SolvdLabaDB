<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.buildingCompany.persistence.dao.IPhoneNumberRepository">
    <select id="getPhoneNumberById" parameterType="java.lang.Long" resultType="PhoneNumber">
        SELECT *
        FROM phone_number
        WHERE id = #{id};
    </select>
</mapper>